    timer = 100
    ldk = "M"
}

if timer &gt; 0 {
    if keyboard_check(ord('O'))and ldk = "M" {
        timer = time
        ldk = "O"
    }
    if keyboard_check(ord('N'))and ldk = "O" {
        timer = time
        ldk = "N"
    }
    if keyboard_check(ord('E'))and ldk = "N" {
        timer = time
        ldk = "E"
    }
    if keyboard_check(ord('Y'))and ldk = "E" {
        timer = time
        ldk = "Y"
        global.money += 1000
        audio_play_sound(snd_buy,true,false)
    }
}

//Munitions Cheat

if keyboard_check(ord('A')) {
    timer = 100
    ldk = "A"
}

if timer &gt; 0 {
    if keyboard_check(ord('M'))and ldk = "A" {
        timer = time
        ldk = "M"
    }
    if keyboard_check(ord('M'))and ldk = "M" {
        timer = time
        ldk = "M"
    }
    if keyboard_check(ord('U'))and ldk = "M" {
        timer = time
        ldk = "U"
        global.munprim += 25
        global.munsec += 500
        audio_play_sound(snd_buy,true,false)
    }
}
