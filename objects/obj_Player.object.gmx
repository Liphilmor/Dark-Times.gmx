            <kind>1</kind>
            <string>///Steuerung
image_index = 0



//Richtung
dirmerk = direction
shot_index = 0

if speed &gt; 0 and keyboard_check(ord("W")) == false
    speed -= 0.04
else if speed &lt; 0 and keyboard_check(ord("S")) == false
    speed += 0.04

MGX = x + lengthdir_x(10,point_direction(x,y,mouse_x,mouse_y) + 50)
MGY = y + lengthdir_y(10,point_direction(x,y,mouse_x,mouse_y) + 50)

MGX2 = x + lengthdir_x(10,point_direction(x,y,mouse_x,mouse_y)  - 50)
MGY2 = y + lengthdir_y(10,point_direction(x,y,mouse_x,mouse_y) - 50)






//Schuss laden
if shot_charge_prim &lt; 1000
    shot_charge_prim += 1

if shot_charge_sec &lt; 1000
    shot_charge_sec += 1

    
    
    
    
//Richtung
direction = dirmerk
//Steuerung
if keyboard_check(ord('D'))
{
    if speed == 0
        dirmerk -= 1
    
    if lastpressed = "w"        
    direction -= 1
    else if lastpressed = "s"
    direction += 1
}

if keyboard_check(ord('A'))
{
    if speed == 0
        dirmerk += 1
    
    if lastpressed = "w"        
    direction += 1
    else if lastpressed = "s"
    direction -= 1
}

if keyboard_check(ord('W'))
{
    lastpressed = "w"
    if speed &lt; max_speed
        speed += 0.05
    else if speed &gt; max_speed
        speed = max_speed
} else {
    if keyboard_check(ord('S')) == false {
    if lastpressed = "w"{
        if speed &lt; 0.2
            speed = 0
    } else {
        if speed &gt; -0.2
            speed = 0
    }
    }
}
if keyboard_check(ord('S'))
{
    
    lastpressed = "s"
    if speed &gt; 0 {
        speed -= 0.05
        image_index = 1
    }
    else if speed &gt; min_speed
        speed -= 0.05
    else if speed &lt; min_speed
        speed = min_speed
}

if keyboard_check(vk_escape)
{
    game_save("WOH.save")
    room_goto(Menu)
}    








//Schiessen

if mouse_check_button_pressed(mb_left)
{   
    if global.ptower = "MiniMG" or global.ptower = "Mini"
    {
        if shot_charge_prim &gt;= 50 and global.munprim &gt; 0
        {
        shot_index = 1
        audio_play_sound(snd_shotsmall, true, 0) //&lt;- Das nur wenn NEw AUdio Engine
        
        id_bullet = instance_create(x,y,obj_bullet)
        id_bullet.direction = tower_dir
        id_bullet.image_angle = tower_dir
        id_bullet.speed = 12
        shot_charge_prim = 0
        global.munprim -= 1
        }
    }
    
    if global.ptower = "Big" or global.ptower = "BigMG"
    {
        if shot_charge_prim &gt;= 100 and global.munprim &gt; 0
        {
        shot_index = 1
        audio_play_sound(snd_shotbig, true, 0) //&lt;- Das nur wenn NEw AUdio Engine
        
        id_bullet = instance_create(x,y,obj_bulletBig)
        id_bullet.direction = tower_dir
        id_bullet.image_angle = tower_dir
        id_bullet.speed = 12
        shot_charge_prim = 0
        global.munprim -= 1
        }
    }
    
}    

if mouse_check_button(mb_right)
{   
    if global.ptower = "MiniMG" or global.ptower = "BigMG"
    {
        if shot_charge_sec &gt;= 5 and global.munsec &gt; 0
        {
        //sound_play(snd_mgshot)
        audio_play_sound(snd_mgshot, true, 0)
        
        id_bullet1 = instance_create(MGX,MGY,obj_MGbullet)
        id_bullet1.direction = tower_dir   //point_direction(MGX,MGY,mouse_x,mouse_y)
        id_bullet1.image_angle = tower_dir   //point_direction(MGX,MGY,mouse_x,mouse_y)
        id_bullet1.speed = 12
        
        id_bullet2 = instance_create(MGX2,MGY2,obj_MGbullet)
        id_bullet2.direction = tower_dir   //point_direction(MGX2,MGY2,mouse_x,mouse_y)
        id_bullet2.image_angle = tower_dir   //point_direction(MGX2,MGY2,mouse_x,mouse_y)
        id_bullet2.speed = 12
        
        shot_charge_sec = 0
        global.munsec -= 2
        }
    }
} 




//Kamera und Kollison


//Mauern Debug Horizontal    
if  place_free(x,y+vspeed) == false
    vspeed = 0   

if place_free(x+hspeed,y) == false
{
  if hspeed &gt; 0
    move_contact_solid(0,-1)
    //speed= -1
  if hspeed &lt; 0
    //speed = 1
    move_contact_solid(180,-1)

  hspeed = 0
}



// Mauern Debug Vertikal

if place_free(x,y+vspeed) == false
{
  if vspeed &gt; 0
    move_contact_solid(270,-1)
    //speed = -1
  if vspeed &lt; 0
    //speed = 1
    move_contact_solid(90,-1)

  vspeed = 0
}
