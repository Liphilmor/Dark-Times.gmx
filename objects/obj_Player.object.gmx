if speed == 0
    direction = dirmerk


view_xview = ((x-view_wview/2) +view_xview*9)/10
view_yview = ((y-view_hview/1.5) +view_yview*9)/10

// Kamera Perspektive
if view_xview &lt; 0
    view_xview = 0
if view_yview &lt; 0
    view_yview = 0
if view_xview &gt; room_width-view_wview
    view_xview = room_width-view_wview  
if view_yview &gt; room_height-view_hview
    view_yview = room_height-view_hview
    
var mdir, dir_dif, dir_speed;

//drehgeschwindigkeit nach Turm
if global.ptower = "Mini" or global.ptower = "MiniMG"
{
dir_speed = 1.5; //Drehgeschwindigkeit
} else if global.ptower = "Big" or global.ptower = "BigMG"
{
dir_speed = 0.8;
}

mdir = point_direction(x,y,mouse_x,mouse_y);
dir_dif = ((((tower_dir - mdir) mod 360) + 540) mod 360) - 180;
if(dir_dif &gt; 0)
{
if(abs(dir_dif) &gt; dir_speed)
tower_dir -= dir_speed;
else
tower_dir = mdir;
}
else
{
if(abs(dir_dif) &gt; dir_speed)
tower_dir += dir_speed;
else
tower_dir = mdir;
}

